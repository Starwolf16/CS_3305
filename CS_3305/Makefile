.PHONY: clean All

All:
	@echo "----------Building project:[ Prct_Thinking_Cap - Debug ]----------"
	@cd "Prct_Thinking_Cap" && "$(MAKE)" -f  "Prct_Thinking_Cap.mk"
clean:
	@echo "----------Cleaning project:[ Prct_Thinking_Cap - Debug ]----------"
	@cd "Prct_Thinking_Cap" && "$(MAKE)" -f  "Prct_Thinking_Cap.mk" clean
